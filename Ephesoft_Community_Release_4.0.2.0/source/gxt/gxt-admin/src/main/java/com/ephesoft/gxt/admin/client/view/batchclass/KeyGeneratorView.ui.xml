<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:form="urn:import:com.sencha.gxt.widget.core.client.form"
	xmlns:coreWidget="urn:import:com.ephesoft.gxt.core.client.ui.widget"
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:VerticalPanel width="100%" height="100%"
		ui:field="keyGeneratorVerticalPanel" styleName="keyVerticalPanel">
		<g:HorizontalPanel width="100%" styleName="keyHorizontalPanel">
			<g:cell horizontalAlignment="ALIGN_RIGHT">
				<g:Label ui:field="batchClassKeyLabel"></g:Label>
			</g:cell>
			<g:cell>
				<g:Label ui:field="batchClassMandatoryField"></g:Label>
			</g:cell>
			<g:cell horizontalAlignment="ALIGN_LEFT">
				<form:TextField ui:field="batchClassKeyText"></form:TextField>
			</g:cell>
		</g:HorizontalPanel>
		<g:HorizontalPanel width="100%" styleName="encryptionAlgoHorizontalPanel"> 
			<g:cell horizontalAlignment="ALIGN_RIGHT">
				<g:Label ui:field="encryptionAlgoLabel"></g:Label>
			</g:cell>
	<!-- <g:cell> -->
	<!-- <g:Label></g:Label> -->
	<!-- </g:cell> -->
			<g:cell  horizontalAlignment="ALIGN_LEFT">
				<coreWidget:ComboBox ui:field="encryptionAlgo"></coreWidget:ComboBox>
			</g:cell>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder>
