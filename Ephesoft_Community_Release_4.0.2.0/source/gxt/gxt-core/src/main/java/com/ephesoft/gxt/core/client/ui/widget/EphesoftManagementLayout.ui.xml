<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container"
	xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client"
	xmlns:coreView="urn:import:com.ephesoft.gxt.core.client.view"
	xmlns:screenNavigator="urn:import:com.ephesoft.gxt.core.client.ui.widget"
	>

	<ui:with
		type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData"
		field="topPanelData">
		<ui:attributes width="1" height="1" />
	</ui:with>

	<container:Viewport>
		<container:SimpleContainer>
			<container:HorizontalLayoutContainer ui:field="horizontalLayoutContainer">
				<screenNavigator:ScreenNavigatorWidget ui:field="screenNavigatorWidget"></screenNavigator:ScreenNavigatorWidget>
			</container:HorizontalLayoutContainer>
		</container:SimpleContainer>
	</container:Viewport>
</ui:UiBinder>